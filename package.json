{
	"name": "ku_prototype",
	"version": "1.0.0",
	"description": "Sitepackage for the project \"KU prototype\" ",
	"main": "index.js",
	"scripts": {
		"lint": "npm-run-all lint:**",
		"lint:js": "eslint --cache",
		"concat:js": "concat-glob-cli -f \"Resources/Public/JavaScript/Src/**/*.js\" -o \"Resources/Public/JavaScript/Dist/ku-scripts.js\"",
		"concat": "npm-run-all concat:js",
		"uglify": "terser --compress passes=2 --toplevel --comments \"/^!/\" --source-map \"root='Resources/Public/JavaScript/Dist',url='Resources/Public/JavaScript/Dist/ku-scripts.min.js.map'\" --output \"Resources/Public/JavaScript/Dist/ku-scripts.min.js\" Resources/Public/JavaScript/Dist/ku-scripts.js",
		"build:js": "npm-run-all lint && npm-run-all concat:js && npm-run-all uglify",
		"build": "npm-run-all build:js",
		"watch:js": "onchange \"Resources/Public/JavaScript/Src/*.js\" -- npm-run-all build:js",
		"watch": "npm-run-all --parallel watch:*"
	},
	"keywords": [
		"TYPO3"
	],
	"author": "Nanna Ellegaard",
	"license": "ISC",
	"devDependencies": {
		"@babel/cli": "^7.18.6",
		"@babel/core": "^7.18.6",
		"@babel/preset-env": "^7.18.6",
		"@popperjs/core": "^2.11.5",
		"@rollup/plugin-babel": "^5.3.1",
		"autoprefixer": "10.4.7",
		"concat-glob-cli": "^0.1.0",
		"concat": "^1.0.3",
		"eslint": "^8.18.0",
		"nodemon": "^2.0.18",
		"npm-run-all": "^4.1.5",
		"onchange": "^7.1.0",
		"stylelint": "^14.9.1",
		"uglify-js": "^3.14.5",
		"terser": "^5.14.1"
	},
	"browserslist": [
		"last 2 versions",
		"not dead"
	],
	"dependencies": {
	}
}
